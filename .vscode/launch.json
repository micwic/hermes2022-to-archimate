{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug: Extraction HERMES2022 (nuextract-client.js)",
      "program": "${workspaceFolder}/hermes2022-concepts-site-extraction/src/nuextract-client.js",
      "cwd": "${workspaceFolder}",
      "env": {
        "NODE_ENV": "development"
      },
      "console": "integratedTerminal",
      "skipFiles": [
        "<node_internals>/**",
        "**/node_modules/**"
      ],
      "outputCapture": "std"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug: Tests BDD - Tous",
      "program": "${workspaceFolder}/node_modules/.bin/jest",
      "args": [
        "--runInBand",
        "--no-cache",
        "--verbose"
      ],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "env": {
        "NODE_ENV": "test"
      }
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug: Tests Extraction (mocked)",
      "program": "${workspaceFolder}/node_modules/.bin/jest",
      "args": [
        "--testPathPatterns=extract-hermes2022-concepts-mocked",
        "--runInBand",
        "--verbose"
      ],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "env": {
        "NODE_ENV": "test"
      }
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug: Tests Extraction (r√©els)",
      "program": "${workspaceFolder}/node_modules/.bin/jest",
      "args": [
        "--testPathPatterns=extract-hermes2022-concepts.steps",
        "--runInBand",
        "--verbose"
      ],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "env": {
        "NODE_ENV": "test"
      }
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug: Tests Template Generation",
      "program": "${workspaceFolder}/node_modules/.bin/jest",
      "args": [
        "--testPathPatterns=template-generation",
        "--runInBand",
        "--verbose"
      ],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "env": {
        "NODE_ENV": "test"
      }
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug: Tests Project Management",
      "program": "${workspaceFolder}/node_modules/.bin/jest",
      "args": [
        "--testPathPatterns=nuextract-project-management",
        "--runInBand",
        "--verbose"
      ],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "env": {
        "NODE_ENV": "test"
      }
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug: Tests Unit (error-handling)",
      "program": "${workspaceFolder}/node_modules/.bin/jest",
      "args": [
        "--testPathPatterns=error-handling",
        "--runInBand",
        "--verbose"
      ],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "env": {
        "NODE_ENV": "test"
      }
    }
  ]
