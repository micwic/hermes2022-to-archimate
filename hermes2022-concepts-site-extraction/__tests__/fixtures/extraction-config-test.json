{
  "hermesVersion": "2022",
  "extractionSource": {
    "baseUrl": "https://www.hermes.admin.ch/en",
    "language": "en",
    "description": "Site de référence en anglais pour l'extraction des concepts HERMES2022"
  },
  "llm": {
    "nuextract": {
      "apiKeyFile": "hermes2022-concepts-site-extraction/config/nuextract-api-key.key",
      "projectName": "HERMES2022",
      "projectDescription": "Project for HERMES2022 concepts extraction by using NuExtact LLM AI",
      "templateTransformationInstructions": {
        "instructions": [
          "- transformes le schéma JSON en template NuExtract",
          "- considères les énumérations JSON comme par exemple \"language\": {\"type\": \"string\",\"enum\": [\"de\", \"fr\", \"it\", \"en\"]} comme des énumérations dans le format de template NuExtract \"language\": [\"de\", \"fr\", \"it\", \"en\"]"
        ]
      },
      "mainJSONConfigurationFile": "shared/hermes2022-extraction-files/config/json-schemas/hermes2022-concepts.json",
      "templateOutputDirectory": "shared/hermes2022-extraction-files/config/nuextract-template-generated",
      "templateReset": true,
      "templateMode": "sync",
      "templateGenerationDuration": 30000,
      "model": "nuextract-large",
      "baseUrl": "nuextract.ai",
      "port": 443,
      "pathPrefix": null,
      "projectsPath": "/api/projects",
      "infer-templatePath": "/api/infer-template",
      "infer-templateAsyncPath": "/api/infer-template-async",
      "infer-textPath": "/api/projects/{projectId}/infer-text",
      "extractPath": "/api/projects/{projectId}/extract",
      "jobsPath": "/api/jobs/{jobId}",
      "timeout": 10
    },
    "anthropic": {
      "enabled": false
    }
  },
  "artifactBaseDir": "shared/hermes2022-extraction-files/data",
  "approvalTargets": [
    "/concepts/overview"
  ],
  "extractionConfigSchemaFile": "hermes2022-concepts-site-extraction/config/extraction-config.schema.json"
}

